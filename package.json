{
  "name": "rapidreview-ui",
  "version": "1.0.0",
  "description": "Rapid Review UI is a collection of HTML, JavaScript and CSS assets that dynamically generate beautiful charts and tables to support literature reviews search step's analysis through visualization.",
  "license": "MIT",
  "author": {
    "name": "Diogo Matheus Costa",
    "email": "dm.matheus@gmail.com",
    "url": "http://diogomatheus.com.br"
  },
  "funding": "https://github.com/sponsors/diogomatheus",
  "keywords": [
    "literature review",
    "rapid review",
    "analysis",
    "visualization"
  ],
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/diogomatheus/rapidreview-ui",
  "repository": {
    "type": "git",
    "url": "https://github.com/diogomatheus/rapidreview-ui"
  },
  "bugs": "https://github.com/diogomatheus/rapidreview-ui/issues",
  "source": "src/index.html",
  "parcelIgnore": [
    "rapidreview-initializer.js"
  ],
  "staticFiles": {
    "staticPath": "src/resource/static"
  },
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "dependencies": {
    "@parcel/service-worker": "^2.8.0",
    "@amcharts/amcharts5": "^5.2.21",
    "@amcharts/amcharts5-fonts": "^5.0.1",
    "@amcharts/amcharts5-geodata": "^5.0.3",
    "@fortawesome/fontawesome-free": "^6.2.0",
    "@popperjs/core": "^2.11.6",
    "ajv": "^8.11.0",
    "bootstrap": "^5.2.0",
    "datatables.net-bs5": "^1.12.1",
    "datatables.net-buttons": "^2.2.3",
    "datatables.net-buttons-bs5": "^2.2.3",
    "datatables.net-responsive-bs5": "^2.3.0",
    "datatables.net-searchpanes-bs5": "^2.0.2",
    "jquery": "^3.6.1",
    "js-yaml": "^4.1.0",
    "jszip": "^3.10.1",
    "pdfmake": "^0.2.5"
  },
  "devDependencies": {
    "@parcel/packager-raw-url": "^2.7.0",
    "@parcel/transformer-inline-string": "^2.7.0",
    "@parcel/transformer-sass": "^2.7.0",
    "@parcel/transformer-webmanifest": "^2.7.0",
    "buffer": "^6.0.3",
    "parcel": "^2.7.0",
    "parcel-reporter-static-files-copy": "^1.4.0",
    "parcel-resolver-ignore": "^2.1.3",
    "path-browserify": "^1.0.1",
    "process": "^0.11.10"
  },
  "scripts": {
    "start": "npx parcel src/index.html",
    "build": "npx parcel build src/index.html --public-url ./"
  }
}
